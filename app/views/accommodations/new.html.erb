<h1>New accommodation</h1>

<% form_for(:accommodation, @accommodation, :url=> yacht_accommodations_path, :html =>{ :class => "new_accommmodation", :id => "new_accommodation" } ) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :content %><br />
    <%= f.text_field :content %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', yacht_accommodations_path %>
