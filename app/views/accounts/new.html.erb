<h1>New account</h1>
<% form_for(@account) do |f| %>
  <%= f.error_messages %>
 
  <p>
    <%= f.label :multiple_listing_system %><br />
    <%= collection_select(:account, :multiple_listing_system_id, MultipleListingSystem.find(:all), :id, :name) %>
  </p>
  <p>
    <%= f.label :login %><br />
    <%= f.text_field :login %>
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', accounts_path %>
