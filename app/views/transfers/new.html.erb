<div id ="left_content">
<% form_for(@upload) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :listing %><br />
    <%= collection_select(:transfer, :listing_id, current_user.listings, :id, :to_s) %>
  </p>

  <p>
    <%= f.label :account %><br />
    <%= collection_select(:transfer, :account_id, current_user.accounts, :id, :to_s) %>
  </p>
  
  <p class="button">
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', transfers_path %>
</div>
